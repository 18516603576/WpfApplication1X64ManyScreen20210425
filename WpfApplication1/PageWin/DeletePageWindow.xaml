<Window x:Class="WpfApplication1.PageWin.DeletePageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="删除页面" Height="300" Width="500">
    <Grid>
        <Label Content="确定删除以下页面吗？" HorizontalAlignment="Center"   VerticalAlignment="Top" Margin="0,77,0,0"/>
        <Label x:Name="pageName"  Content="未获取到要删除的页面" HorizontalAlignment="Center" Margin="0,119,0,0" VerticalAlignment="Top" Foreground="#FFF50404"/>
        <Button Content="确定" Name="Submit_Button" HorizontalAlignment="Left" Margin="143,186,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="Submit_Button_Click"/>
        <Button Content="取消" Name="Cancel_Button" HorizontalAlignment="Left" Margin="287,186,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="Cancel_Button_Click"/>

    </Grid>
</Window>
