<Window x:Class="WpfApplication1.NewPageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="新建子页面" Height="500" Width="800">
    <Grid>

        <Label Content="页面名称" HorizontalAlignment="Left" Margin="237,97,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.588,-1.164"/>
        <TextBox Name="pagename" HorizontalAlignment="Left" VerticalContentAlignment="Center" Padding="5" Height="30" Margin="237,127,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="205"/>
        <Label Content="页面切换方式" HorizontalAlignment="Left" Margin="237,177,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.588,-1.164"/>


        <Button Content="保存" HorizontalAlignment="Left" Margin="617,398,0,0" VerticalAlignment="Top" Width="96" Click="Button_Click" Height="26" RenderTransformOrigin="4.213,7.415"/>
        <ComboBox x:Name="pageSwitchType" HorizontalAlignment="Left" Margin="237,208,0,0" Padding="8" VerticalAlignment="Top" Width="205" Height="30" />

    </Grid>
</Window>
