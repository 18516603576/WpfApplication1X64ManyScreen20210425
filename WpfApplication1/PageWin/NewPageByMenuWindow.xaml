        <Button Content="保存" HorizontalAlignment="Left" Margin="638,405,0,0" VerticalAlignment="Top" Width="96" Click="Button_Click" Height="26" RenderTransformOrigin="4.213,7.415"/>
        <ComboBox x:Name="parentIdComboBox" HorizontalAlignment="Left" Margin="307,147,0,0" VerticalAlignment="Top" Width="131" RenderTransformOrigin="0.321,4.812" Height="30" Padding="8"/>
        <Label Content="归属" HorizontalAlignment="Left" Margin="253,147,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.588,-1.164"/>
        <Label Content="页面切换方式" HorizontalAlignment="Left" Margin="205,223,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.588,-1.164"/>
        <ComboBox x:Name="pageSwitchType" HorizontalAlignment="Left" Margin="307,219,0,0" Padding="8" VerticalAlignment="Top" Width="205" Height="30" />

    </Grid>
</Window>
