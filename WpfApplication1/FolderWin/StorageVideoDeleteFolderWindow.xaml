<Window x:Class="WpfApplication1.FolderWin.StorageVideoDeleteFolderWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1.FolderWin"
        mc:Ignorable="d"
        Title="删除文件夹" Height="300" Width="500">
    <Grid>
        <Label Content="确定删除以下文件夹吗？" HorizontalAlignment="Center"   VerticalAlignment="Top" Margin="0,77,0,0"/>
        <Label x:Name="pageName"  Content="未获取到要删除的文件夹" HorizontalAlignment="Center" Margin="0,119,0,0" VerticalAlignment="Top" Foreground="#FFF50404"/>
        <Button Content="确定" Name="Submit_Button" HorizontalAlignment="Left" Margin="143,186,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="Submit_Button_Click"/>
        <Button Content="取消" Name="Cancel_Button" HorizontalAlignment="Left" Margin="287,186,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="Cancel_Button_Click"/>

    </Grid>
</Window>
