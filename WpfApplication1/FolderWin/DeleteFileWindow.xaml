<Window x:Class="WpfApplication1.FolderWin.DeleteFileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1.FolderWin"
        mc:Ignorable="d"
          Title="删除文件" Height="300" Width="500">
    <Grid>
        <Label x:Name="deleteInfo"  Content="未获取到要删除的文件" HorizontalAlignment="Center" Margin="167,82,136,0" VerticalAlignment="Top" Foreground="#FFF50404" Height="29" Width="189"/>
        <Button Content="确定" Name="Submit_Button" HorizontalAlignment="Left" Margin="112,160,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="Submit_Button_Click"/>
        <Button Content="取消" Name="Cancel_Button" HorizontalAlignment="Left" Margin="256,160,0,0" VerticalAlignment="Top" Width="100" Height="30" Click="Cancel_Button_Click"/>

    </Grid>
</Window>
